{
	"name": "@default-js/defaultjs-common-utils",
	"buildname": "defaultjs-common-utils",
	"version": "0.15.2",
	"description": "common utils for javascript. Utils to get the global scope (browser, webworker & co.). Customizable string escaper. Utilitize for objects. Extension at base javascript api (usefull extention like String.hashcode()).",
	"main": "./index.js",
	"scripts": {
		"build-only": "webpack --config webpack.prod.js && webpack --config webpack.dev.js",
		"build": "npm test && webpack --config webpack.prod.js && webpack --config webpack.dev.js",
		"//dev:live": "webpack-dev-server --open --config webpack.dev-server.js && npm run test:live",
		"dev:live": "webpack serve --config webpack.dev-server.js --open",
		"test:live": "karma start karma.live.js",
		"test:integration": "karma start karma.integration.js",
		"test": "karma start karma.test.js"
	},
	"keywords": [
		"defaultjs",
		"common",
		"utils",
		"object helper",
		"string escaper"
	],
	"author": "Frank Sch√ºler",
	"license": "MIT",
	"devDependencies": {
		"karma": "^6.3.4",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage": "^2.0.3",
		"karma-firefox-launcher": "^2.1.1",
		"karma-html2js-preprocessor": "^1.1.0",
		"karma-jasmine": "^4.0.1",
		"karma-safari-launcher": "^1.0.0",
		"karma-sourcemap-loader": "^0.3.8",
		"karma-webpack": "^5.0.0",
		"puppeteer": "^10.0.0",
		"replace-in-file-webpack-plugin": "^1.0.6",
		"webpack": "^5.40.0",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.8.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/default-js/defaultjs-common-utils.git"
	},
	"bugs": {
		"url": "https://github.com/default-js/defaultjs-common-utils/issues"
	},
	"homepage": "https://github.com/default-js/defaultjs-common-utils#readme",
	"directories": {
		"test": "test"
	},
	"files": [
		"index.js",
		"browser.js",
		"src/**",
		"LICENSE",
		"README.md"
	]
}
